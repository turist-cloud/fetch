{
  "compilerOptions": {
    "declaration": true,
    "esModuleInterop": true,
    "lib": ["dom","esnext"],
    "module": "CommonJS",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "removeComments": true,
    "strict": true,
    "target": "es2022",
    "inlineSourceMap": true,
    "types": ["jest","node"],
    "typeRoots": [
      "./node_modules/@types"
    ]
  },
  "include": [
    "./src"
  ]
}
